{
  "name": "myapp-backend",
  "version": "1.0.0",
  "description": "Backend pour l'application Flutter de gestion d'entreprises",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node test-with-reports.js",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage; node ../generate-test-reports.js",
    "test:verbose": "jest --verbose; node ../generate-test-reports.js",
    "test:auth": "node test_auth.js",
    "test:login-type": "node test_login_type_compte.js",
    "debug": "node debug_auth.js",
    "migrate": "node migration_add_type_compte.js",
    "migrate:verify": "node migration_add_type_compte.js verify",
    "migrate:cleanup": "node migration_add_type_compte.js cleanup",
    "post-migrate": "node post_migration.js",
    "vercel-build": "echo 'No build step required for server'"
  },
  "keywords": [
    "flutter",
    "backend",
    "mongodb",
    "express",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@huggingface/inference": "^4.11.1",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "diskusage": "^1.2.0",
    "dotenv": "^17.2.2",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "faiss-node": "^0.5.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.35",
    "lodash": "^4.17.21",
    "mongoose": "^8.18.0",
    "multer": "^1.4.5-lts.1",
    "natural": "^8.1.0",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.9.8",
    "pdfkit": "^0.17.2",
    "pidusage": "^4.0.1",
    "socket.io": "^4.8.1",
    "tesseract.js": "^4.1.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "mongodb-memory-server": "^10.2.3",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.4"
  }
}
